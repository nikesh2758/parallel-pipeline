pipeline {
  agent none
  stages {
    stage ('build') {
    agent { label 'java'}
       steps {
               sh '''
                  echo "this is build stage"
                  sleep 2
             '''
             }
    }
    parallel {
    stages ('deploy') {
    stage ('deploy1') {
    agent {label 'java'}
       steps {
              sh '''
                  echo "this is deploy stage"
                  sleep 2
             '''
             }
    }
    stage ('deploy2') {
    agent {label 'cprog'}
       steps {
                   sh '''
                  echo "this is deploy stage"
                  sleep 2
             '''
             }
    }
    }
    }
    stage ('test') {
    agent {label 'cprog'}
       steps {
             sh '''
                  echo "this is deploy stage"
                  sleep 2
             '''
             }
    }
  }
}  
