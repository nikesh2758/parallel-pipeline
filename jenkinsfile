pipeline {
  agent none
  stages {
    stage ('build') {
    agent java
       steps {
               sh '''
                  echo "this is build stage"
                  sleep 2
             '''
             }
    }
    stage ('deploy1') {
    agent master
       steps {
              sh '''
                  echo "this is deploy stage"
                  sleep 2
             '''
             }
    }
    stage ('deploy2') {
    agent master
       steps {
                   sh '''
                  echo "this is deploy stage"
                  sleep 2
             '''
             }
    }
    stage ('test') {
    agent cprog
       steps {
             sh '''
                  echo "this is deploy stage"
                  sleep 2
             '''
             }
    }
  }
}  
